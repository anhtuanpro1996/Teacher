{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "amd": true,
    "jquery": true
  },
  "globals": {
    "_": true,
    "React": true,
    "ReactWidgets": true,
    "Router": true,
    "App": true,
    "window": true,
    "document": true,
    "module": true,
    "CommonFunction": true,
    "localStorage": true,
    "confirm": true,
    "moment": true,
    "location": true
  },
  "extends": ["plugin:react/recommended"],
  "rules": {
    "indent": ["error", 2]
  }
}
